    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.vue src"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "3.2.33",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0",
    "pinia": "^2.0.14"
  },
  "devDependencies": {
    "@ehutch79/vite-eslint": "0.0.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/eslint-config-prettier": "^6.0.0",

    "crypto-js": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "vite-plugin-mock": "^2.3.0",

    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^1.3.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",

    "@vue/compiler-sfc": "^3.1.2",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "element-plus": "^2.2.13",
    "vue-i18n": "^9.0.0",
    "mockjs": "^1.1.0",
    "sass": "^1.41.1",
    "vite": "^2.3.7",
    "vite-plugin-svg-icons": "^0.4.0"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "git add"
