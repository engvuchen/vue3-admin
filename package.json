{
  "name": "vue3-element-admin",
  "version": "1.0.0",
  "author": {
    "name": "engvuchen",
    "url": "https://github.com/engvuchen"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "vite build",
    "build:mock": "vite build --mode mock",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.vue src",
    "prepare": "husky"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "pinia": "^2.0.14",
    "vue": "^3.4.21",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.2",
    "autoprefixer": "^10.2.5",
    "crypto-js": "^4.0.0",
    "element-plus": "^2.2.13",
    "eslint": "^8.35.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.41.1",
    "vite": "^2.3.7",
    "vite-plugin-svg-icons": "^0.4.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-i18n": "^9.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ],
    "*.{js,vue,json,css,html,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT"
}
